      ],
      "description": "Declare a boolean variable and print it."
    },
    "If Else Statement": {
      "prefix": "ifelse",
      "body": [
        "sahichai (${1:condition}) {",
        "\t${2:// code}",
        "} galatchai {",
        "\t${3:// else code}",
        "}"
      ],
      "description": "If-Else statement in CuttingChai."
    }
  }
  